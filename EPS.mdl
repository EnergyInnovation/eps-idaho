Achieved Capacity Factors[Electricity Source]=
	MAX(0,
	 SUM(Achieved Capacity Factors by Hour[Electricity Timeslice!,Hour!,Electricity Source\
		]
	     * SHELF Days per Electricity Timeslice[Electricity Timeslice!]
	     * One Hour)
	 /Hours per Year
	)
	~	Dmnl
	~		|

Achieved Capacity Factors by Hour[Electricity Timeslice,Hour,Electricity Source]=
	ZIDZ(Total Electricity Dispatched by Hour by Plant Type[Electricity Timeslice,Hour,Electricity Source\
		],
	     Electricity Generation Capacity[Electricity Source]*One Hour)
	~	Dmnl
	~		|

Achieved Capacity Factors for New Plants x Hours per Year[Electricity Source]=
	QUANTUM(
	  IF THEN ELSE(
	      ABS(Last Year Electricity Generation Capacity[Electricity Source])*MW Tolerance Unit Correction Factor\
		 < Tolerance
	,
	
	      Expected Capacity Factor x Hours per Year for Plants Built This Year[Electricity Source\
		],
	
	      Expected Capacity Factors for Plants Built This Year[Electricity Source]
	      * Three Year Average Fraction of Expected Capacity Factors Achieved[Electricity Source\
		]
	      * Hours per Year
	    ),
	  1
	 )
	~	hours
	~		|

